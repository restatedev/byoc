{
  "name": "@restatedev/byoc",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "download-restatectl": "curl -L https://restate.gateway.scarf.sh/v1.3.0-rc.1/restatectl-aarch64-unknown-linux-musl.tar.xz | tar -zx --strip-components=1 -C lib/lambda/restatectl restatectl-aarch64-unknown-linux-musl/restatectl && mv lib/lambda/restatectl/restatectl lib/lambda/restatectl/bootstrap",
    "build": "tsc && mkdir -p ./dist/lambda/restatectl && cp ./lib/lambda/restatectl/bootstrap ./dist/lambda/restatectl/bootstrap",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch": "^3.782.0",
    "@aws-sdk/client-ec2": "^3.783.0",
    "@aws-sdk/client-ecs": "^3.778.0",
    "@aws-sdk/client-lambda": "^3.782.0",
    "@types/aws-lambda": "^8.10.148",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "^2.171.1",
    "esbuild": "^0.25.2",
    "jest": "^29.7.0",
    "jest-cdk-snapshot": "^2.2.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.171.1",
    "constructs": "^10.0.0"
  }
}
